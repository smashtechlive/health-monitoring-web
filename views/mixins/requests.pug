div(class='grid')
    .cell.th.grey-bg
        p UTC Time
    .cell.th.grey-bg
        p Name
    .cell.th.grey-bg
        p Value
    .cell.th.grey-bg
        p Source
	 
div(class='wrapper') 
    - for(var i = 0; i < res.length; i++)
        div(class='cell')
            p #{res[i].utcDateTime}
        div(class='cell')
            p #{res[i].field.name}
        div(class='cell')
            p #{res[i].field.value}
        div(class='cell')
            p #{res[i].source}	
